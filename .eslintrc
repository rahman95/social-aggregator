{
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
          "globalReturn": true
      }
    },
    "rules": {
      "no-console": 0,
      "no-restricted-globals": 0,
      "data.mpan_core": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "import/newline-after-import": 0,
      "no-plusplus": 0
    }
  }